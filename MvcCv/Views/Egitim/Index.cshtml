@using MvcCv.Models.Entity
@model List<TBLEgitimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-" style="margin-top:50px;">
    <tr>
        <th>Id</th>
        <th>Başlık</th>
        <th>Alt Başlık 1</th>
        <th>Alt Başlık 2</th>
        <th>GANO</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Baslik</td>
                <td>@item.AltBaslik1</td>
                <td>@item.AltBaslik2</td>
                <td>@item.GNO</td>
                <td>@item.Tarih</td>
                <td><a class="btn btn-danger btn-sm" href="/Egitim/EgitimSil/@item.Id"><i class="fas fa-trash"></i>Sil</a></td>
                <td><a class="btn btn-info btn-sm" href="/Egitim/EgitimGüncelle/@item.Id"> <i class="fas fa-pencil-alt"></i>Güncelle</a></td>
            </tr>
        }
        
    </tbody>
    
</table>
<div>
    <a href="/Egitim/EgitimEkle/" class="btn btn-info">Ekle</a>
</div>